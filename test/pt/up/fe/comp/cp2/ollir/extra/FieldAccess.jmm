import io;

class FieldAccess {
    int counter;

    public int increment(int val) {
        counter = counter + val;
        return counter;
    }

    public int getValue() {
        return counter;
    }

    public static void main(String[] args) {
        FieldAccess fa;
        int temp;
        fa = new FieldAccess();



        fa.increment(3);
        temp = fa.getValue();
        io.print(temp);
    }
}
