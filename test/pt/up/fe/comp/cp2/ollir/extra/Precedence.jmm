import io;

class Precedence {
    public static void main(String[] args) {
        int a;
        int b;
        int c;

        a = 2 + 3 * 4;
        io.print(a);

        b = (2 + 3) * 4;
        io.print(b);

        c = a + 5 < b * 2;
        if (c) {
            io.print(1);
        } else {
            io.print(0);
        }
    }
}
